{
  "name": "Ethernet_Manager_STM32",
  "version": "1.0.1",
  "description": "Simple Ethernet Manager for STM32F/L/H/G/WB/MP1 boards with Ethernet W5x00, ENC28J60 or built-in LAN8742A shields, with or without SSL, configuration data saved in EEPROM. With DoubleResetDetect feature.",
  "keywords": "communication, ethernet, data, http, W5500, W5x00, ENC28J60, STM32, Credentials, Manager, Config Portal, Double, Reset, double-reset, Detector, MQTT, DynamicParameters",
  "authors":
  {
    "name":  "Khoi Hoang",
    "url":   "https://github.com/khoih-prog",
    "maintainer": true
  },
  "repository":
  {
    "type": "git",
    "url": "//https://github.com/khoih-prog/Ethernet_Manager_STM32"
  },
  "homepage": "https://github.com/khoih-prog/Ethernet_Manager_STM32",
  "export": {
    "exclude": [
      "linux",
      "extras",
      "tests"
    ]
  },
  "dependencies": 
  [
    {
      "owner": "khoih-prog",
      "name": "EthernetWebServer_STM32",
      "version": ">=1.1.0",
      "platforms": ["*"]
    },
    {
      "owner": "khoih-prog",
      "name": "DoubleResetDetector_Generic",
      "version": ">=1.0.2",
      "platforms": ["*"]
    },
    {
      "owner": "khoih-prog",
      "name": "Functional-Vlpp",
      "version": ">=1.0.1",
      "platforms": ["*"]
    },
    {
      "owner": "stm32duino",
      "name": "STM32duino LwIP",
      "version": "^2.1.2",
      "platforms": "ststm32"
    },
    {
      "owner": "stm32duino",
      "name": "STM32Ethernet",
      "version": "^1.2.0",
      "platforms": "ststm32"
    }
  ],
  "frameworks": "*",
  "platforms": "*",
  "examples": "examples/*/*/*.ino"
}
